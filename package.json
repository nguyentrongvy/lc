{
  "name": "vnlp-live-chat",
  "version": "0.0.1",
  "description": "Init source nodejs",
  "main": "index.js",
  "scripts": {
    "start": "node bin/index.js",
    "test": "mocha --timeout 10000",
    "lint": "eslint . --ext .js --ignore-path .gitignore --fix"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  },
  "dependencies": {
    "@hapi/joi": "^15.0.1",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "bull": "^3.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "express-validation": "^1.0.2",
    "helmet": "^3.21.1",
    "jaeger-client": "^3.18.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "mongoose": "^5.5.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.1.1",
    "redis": "^3.0.2",
    "socket.io": "^2.2.0",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0"
  },
  "keywords": [
    "live-chat"
  ],
  "author": "DuyN"
}
